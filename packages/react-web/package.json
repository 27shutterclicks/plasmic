{
  "name": "@plasmicapp/react-web",
  "version": "0.2.89",
  "description": "plasmic library for rendering in the presentational style",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/react-web.esm.js",
  "size-limit": [
    {
      "path": "dist/react-web.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-web.esm.js",
      "limit": "10 KB"
    }
  ],
  "files": [
    "dist",
    "lib",
    "skinny"
  ],
  "scripts": {
    "build": "tsdx build && rollup -c && mkdir -p lib && cp src/styles/plasmic.css lib/",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {},
  "dependencies": {
    "classnames": "^2.2.6",
    "react-aria": "^3.7.0",
    "react-stately": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@react-types/overlays": "^3.4.0",
    "@react-types/select": "^3.2.0",
    "@react-types/shared": "^3.5.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.47.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
