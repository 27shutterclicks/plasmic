{
  "name": "@plasmicapp/loader-react",
  "version": "1.0.16",
  "module": "dist/loader-react.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "host"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && rollup -c",
    "yalcp": "yalc publish --push",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@plasmicapp/host": "0.0.16",
    "@plasmicapp/loader-core": "1.0.8",
    "@plasmicapp/react-web": "^0.2.68",
    "pascalcase": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/loader-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/loader-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/pascalcase": "^1.0.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "license": "MIT",
  "gitHead": "fa53f7d79f0e26d8b061102fda0c06788da6f8a7"
}
