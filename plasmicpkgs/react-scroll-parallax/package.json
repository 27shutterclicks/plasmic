{
  "name": "@plasmicpkgs/react-scroll-parallax",
  "version": "0.0.2",
  "description": "Plasmic registration call for the HTML5 video element",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/react-scroll-parallax.esm.js",
  "files": [
    "dist",
    "ParallaxWrapper"
  ],
  "size-limit": [
    {
      "path": "dist/react-scroll-parallax.cjs.production.min.js",
      "limit": "40 KB"
    },
    {
      "path": "dist/react-scroll-parallax.esm.js",
      "limit": "40 KB"
    }
  ],
  "scripts": {
    "build": "tsdx build && rollup -c",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/node": "^14.0.26",
    "rollup": "^2.47.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@plasmicapp/host": "^0.0.32",
    "react-scroll-parallax": "^2.4.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
